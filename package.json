{
  "name": "@teamprodevs/postgrest-js",
  "version": "0.0.7",
  "description": "Isomorphic PostgREST client",
  "keywords": ["postgrest", "supabase"],
  "license": "MIT",
  "author": "Supabase",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smarts-uz/postgrest-js.git"
  },
  "files": ["dist"],
  "main": "dist/index.js",      
  "module": "dist/index.mjs",   
  "types": "dist/index.d.ts",    
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --dts --format cjs,esm --out-dir dist",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@supabase/node-fetch": "^2.6.14"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tsup": "^8.5.0",
    "typescript": "^4.5.5"
  }
}
