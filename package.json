{
  "name": "@teamprodevs/postgrest-js",
  "version": "0.0.3",
  "description": "Isomorphic PostgREST client",
  "keywords": [
    "postgrest",
    "supabase"
  ],
  "license": "MIT",
  "author": "Supabase",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smarts-uz/postgrest-js.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.json --module commonjs --outDir dist/cjs",
    "build:esm": "tsc -p tsconfig.json --module esnext --outDir dist/esm",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly --declaration --outDir dist/types",
    "build": "run-s clean build:cjs build:esm build:types",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@supabase/node-fetch": "^2.6.14"
  },
  "devDependencies": {
    "typescript": "^4.5.5",
    "rimraf": "^3.0.2",
    "npm-run-all": "^4.1.5"
  }
}
